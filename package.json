{
  "name": "rnw-front-end-development",
  "version": "0.9.1",
  "description": "ResearchNow front-end application for NYPL's newest search & retrieval platform.",
  "author": "NYPL Digital",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "eb-start": "npm run dist && node index",
    "local-start": "APP_ENV=local NODE_ENV=development npm start",
    "dev-start": "APP_ENV=development NODE_ENV=development npm start",
    "prod-start": "APP_ENV=development NODE_ENV=production npm start",
    "dist": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.babel.js",
    "rebuild-modules": "rm -rf node_modules && npm install",
    "postinstall": "npm run dist",
    "test": "APP_ENV=production BABEL_ENV=test ./node_modules/.bin/nyc --require mocha npm run test-unit",
    "test-unit": "mocha test/helpers/browser.js test/unit",
    "coverage-report": "BABEL_ENV=test ./node_modules/.bin/nyc report --reporter=lcov",
    "coverage-report-html": "BABEL_ENV=test ./node_modules/.bin/nyc report --reporter=html && open -a 'Google Chrome' ./coverage/index.html",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL/sfr-bookfinder-front-end"
  },
